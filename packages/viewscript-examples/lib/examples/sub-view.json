      "attributes": {
      }
    },
    {
      "id": "pizzaType",
      "type": "select",
      "attributes": {
        "heading": "Pizza",
        "mandatory": true,
        "titleMap": [
          {
            "value": "HAWAIIAN",
            "title": "The Hawaiian"
          },
          {
            "value": "PEPPERONI",
            "title": "Pepperoni"
          },
          {
